{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "Launch Chrome",
          "url": "http://localhost:5000",
          "webRoot": "${workspaceFolder}/public/",
          "sourceMapPathOverrides": {
              "src/*": "${workspaceFolder}/src/*"
          },
          "preLaunchTask": "watch",
          "port": 9222
      }
  ]
}